<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<style>body { font-family: DejaVu Sans }</style>
<body>
<table align="center" border="0" style="width: 100%">
    <tr>
        <td colspan="6" align="center" style="background: rgb(13,110,253)"><br><br></td>
    </tr>
    <tr>
        <td colspan="6" align="left">
            <h3>EShop</h3>
            <p>(413) 593-1356</p>
            <p>63 Mitchell Dr</p>
            <p>Chicopee, Massachusetts(MA), 01022</p>
        </td>
    </tr>
    <tr>
        <td>
            <br>
            <br>
        </td>
    </tr>
    <tr>
        <td>
            <h3>{{ "Bill to"|trans }}</h3>
            <p>{{ order.firstName }} {{ order.lastName }}</p>
            <p>{{ order.country }} {{ order.city }}</p>
            <p>{{ order.line1 }} {{ order.line2 }}</p>
        </td>
        <td>
            <h3>{{ "Ship to"|trans }}</h3>
            <p>{{ order.firstName }} {{ order.lastName }}</p>
            <p>{{ order.country }} {{ order.city }}</p>
            <p>{{ order.line1 }} {{ order.line2 }}</p>
        </td>
        <td>
            <h3>{{ "Invoice"|trans }} #</h3>
            <h3>{{ "Invoice date"|trans }}</h3>
        </td>
        <td>
            <p>US-{{ order.id }}</p>
            <p>{{ order.createdAt| date ('d/m/Y') }}</p>
        </td>
    </tr>
    <tr>
        <td>
            <br>
            <br>
        </td>
    </tr>
    <tr>
        <td colspan="4" align="left" style="border-top: solid 2px black; border-bottom: solid 2px black">
            <h1>{{ "Invoice total"|trans }}</h1>
        </td>
        <td colspan="2" align="right" style="border-top: solid 2px black; border-bottom: solid 2px black">
            <h1>{{ order.grandTotal }}</h1>
        </td>
    </tr>
    <tr>
        <td>
            <br>
            <br>
        </td>
    </tr>
    <tr>
        <td>
            <h3>{{ "QTY" }}</h3>
        </td>
        <td>
            <h3>{{ "Product name" }}</h3>
        </td>
        <td>
            <h3>{{ "Unit price" }}</h3>
        </td>
        <td>
            <h3>{{ "Amount" }}</h3>
        </td>
        {% set number = 0 %}
        {% for item in order.items %}
        {% set number = number + 1 %}
    <tr>
        <td>{{ number }}</td>
        <td>{{ item.product.title }}</td>
        <td>{{ item.price }}</td>
        <td>{{ item.quantity }}</td>
    </tr>
    {% endfor %}
    <tr>
        <td>
            <br>
            <br>
        </td>
    </tr>
    <tr>
        <td colspan="2"></td>
        <td>
            <p>{{ "Subtotal"|trans }}</p>
            <p>{{ "Sales tax"|trans }}</p>
        </td>
        <td>
            <p>{{ order.grandTotal }}</p>
            <p>{{ order.tax }}</p>
        </td>
    </tr>
    <tr>
        <td colspan="4" align="right">
            <img style="max-height: 100px" src="data:image/jpeg;base64,">
        </td>
    </tr>
    <tr>
        <td>
            <br>
            <br>
            <br>
            <br>
            <br>
            <br>
            <br>
            <br>
        </td>
    </tr>
    <tr>
        <td>
            <h3>
                {{ "TERMS & CONDITIONS"|trans }}
            </h3>
            <p>
                {{ "Payment is due within 15 days"|trans }}
            </p>
            <p>
                {{ "Please make checks payable to: EShop"|trans }}
            </p>
        </td>
    </tr>
    <tr>
        <td colspan="6" align="center" style="background: rgb(13,110,253)"><br><br></td>
    </tr>

</table>
</body>
</html>